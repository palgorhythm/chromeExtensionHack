{
  "manifest_version": 2,
  "name": "introspect",
  "version": "0.1",
  "description": "introspecify ur life",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "exclude_matches":["*://www.google.com/*"],
  "permissions": ["storage"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["main.js"],
      "css": ["index.css"]
    }
  ],
  "browser_action": {
    "default_icon": "faceImg.jpg",
    "default_title": "introspect",
    "default_popup": "index.html"
  }
}
